:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x560e627a9d70 .scope module, "test_twos_complement_inv" "test_twos_complement_inv" 2 4;
 .timescale -9 -9;
v0x560e627ca550_0 .var "inp", 3 0;
v0x560e627ca610_0 .net "out", 3 0, L_0x560e627ca970;  1 drivers
S_0x560e627a9ef0 .scope module, "uut" "twos_complement_inv" 2 8, 3 13 0, S_0x560e627a9d70;
 .timescale -9 -9;
    .port_info 0 /INPUT 4 "twos_in"
    .port_info 1 /OUTPUT 4 "inverse_out"
P_0x560e627a5f20 .param/l "WIDTH" 0 3 15, +C4<00000000000000000000000000000100>;
L_0x560e627a38f0 .functor NOT 4, L_0x560e627ca710, C4<0000>, C4<0000>, C4<0000>;
L_0x7f17808d3018 .functor BUFT 1, C4<0001>, C4<0>, C4<0>, C4<0>;
v0x560e627aa0c0_0 .net/2u *"_s0", 3 0, L_0x7f17808d3018;  1 drivers
v0x560e627ca060_0 .net *"_s5", 0 0, L_0x560e627ca880;  1 drivers
v0x560e627ca140_0 .net *"_s6", 3 0, L_0x560e627a38f0;  1 drivers
v0x560e627ca200_0 .net "inverse_out", 3 0, L_0x560e627ca970;  alias, 1 drivers
v0x560e627ca2e0_0 .net "temporary", 3 0, L_0x560e627ca710;  1 drivers
v0x560e627ca410_0 .net "twos_in", 3 0, v0x560e627ca550_0;  1 drivers
E_0x560e627a2b70 .event edge, v0x560e627ca410_0, v0x560e627ca200_0;
L_0x560e627ca710 .arith/sub 4, v0x560e627ca550_0, L_0x7f17808d3018;
L_0x560e627ca880 .part v0x560e627ca550_0, 3, 1;
L_0x560e627ca970 .functor MUXZ 4, v0x560e627ca550_0, L_0x560e627a38f0, L_0x560e627ca880, C4<>;
    .scope S_0x560e627a9ef0;
T_0 ;
    %wait E_0x560e627a2b70;
    %load/vec4 v0x560e627ca410_0;
    %load/vec4 v0x560e627ca200_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 3 25 "$display", "*** ERROR *** Cannot convert a positive number" {0 0 0};
T_0.0 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x560e627a9d70;
T_1 ;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x560e627ca550_0, 0, 4;
    %delay 1, 0;
    %load/vec4 v0x560e627ca610_0;
    %cmpi/ne 1, 0, 4;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 14 "$display", "Expecting %b, got %b", v0x560e627ca550_0, v0x560e627ca610_0 {0 0 0};
T_1.0 ;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x560e627ca550_0, 0, 4;
    %delay 1, 0;
    %load/vec4 v0x560e627ca610_0;
    %cmpi/ne 1, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %vpi_call 2 21 "$display", "Expecting %b, got %b", v0x560e627ca550_0, v0x560e627ca610_0 {0 0 0};
T_1.2 ;
    %vpi_call 2 24 "$display", "Test Two's Invert finished" {0 0 0};
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
