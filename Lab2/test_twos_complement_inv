#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x56002776c1b0 .scope module, "test_twos_complement_inv" "test_twos_complement_inv" 2 4;
 .timescale -9 -9;
v0x56002778c830_0 .var "inp", 3 0;
v0x56002778c920_0 .net "out", 3 0, L_0x560027766bb0;  1 drivers
S_0x56002776c330 .scope module, "uut" "twos_complement_inv" 2 8, 3 2 0, S_0x56002776c1b0;
 .timescale -9 -9;
    .port_info 0 /INPUT 4 "twos_in"
    .port_info 1 /OUTPUT 4 "inverse_out"
P_0x56002776c500 .param/l "WIDTH" 0 3 4, +C4<00000000000000000000000000000100>;
L_0x560027766bb0 .functor BUFZ 4, v0x56002778c600_0, C4<0000>, C4<0000>, C4<0000>;
v0x56002776c5a0_0 .net "inverse_out", 3 0, L_0x560027766bb0;  alias, 1 drivers
v0x56002778c600_0 .var "temporary", 3 0;
v0x56002778c6e0_0 .net "twos_in", 3 0, v0x56002778c830_0;  1 drivers
E_0x560027766f60 .event edge, v0x56002778c6e0_0;
    .scope S_0x56002776c330;
T_0 ;
    %wait E_0x560027766f60;
    %load/vec4 v0x56002778c6e0_0;
    %parti/s 1, 3, 3;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.0, 4;
    %vpi_call 3 12 "$display", "*** ERROR *** Cannot convert a positive number" {0 0 0};
    %load/vec4 v0x56002778c6e0_0;
    %store/vec4 v0x56002778c600_0, 0, 4;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x56002778c6e0_0;
    %subi 1, 0, 4;
    %inv;
    %store/vec4 v0x56002778c600_0, 0, 4;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x56002776c1b0;
T_1 ;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x56002778c830_0, 0, 4;
    %delay 1, 0;
    %load/vec4 v0x56002778c920_0;
    %cmpi/ne 1, 0, 4;
    %jmp/0xz  T_1.0, 4;
    %vpi_call 2 17 "$display", "Expecting %b, got %b", v0x56002778c830_0, v0x56002778c920_0 {0 0 0};
T_1.0 ;
    %vpi_call 2 19 "$display", "Test Two's Invert finished" {0 0 0};
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_twos_complement_inv.v";
    "twos_complement_inv.v";
