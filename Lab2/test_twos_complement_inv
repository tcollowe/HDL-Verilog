    .scope S_0x55df53743d70;
T_1 ;
    %pushi/vec4 11, 0, 4;
    %store/vec4 v0x55df53764550_0, 0, 4;
    %delay 1, 0;
    %load/vec4 v0x55df53764610_0;
    %cmpi/ne 1, 0, 4;
