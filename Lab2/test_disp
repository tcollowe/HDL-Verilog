#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5604d7da4900 .scope module, "test_disp" "test_disp" 2 4;
 .timescale -9 -9;
v0x5604d7da42a0_0 .var "inp", 15 0;
S_0x5604d7da4a80 .scope module, "display" "disp" 2 7, 3 1 0, S_0x5604d7da4900;
 .timescale -9 -9;
    .port_info 0 /INPUT 16 "inp"
v0x5604d7da3cd0_0 .net "inp", 15 0, v0x5604d7da42a0_0;  1 drivers
    .scope S_0x5604d7da4a80;
T_0 ;
    %vpi_call 3 5 "$monitor", "decimal %-8d hex %06h octal %o binary %b", v0x5604d7da3cd0_0, v0x5604d7da3cd0_0, v0x5604d7da3cd0_0, v0x5604d7da3cd0_0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5604d7da4900;
T_1 ;
    %pushi/vec4 63921, 0, 16;
    %store/vec4 v0x5604d7da42a0_0, 0, 16;
    %vpi_call 2 11 "$display", "Should be: decimal 63921    hex 00f9b1 octal 174661 binary 1111100110110001" {0 0 0};
    %vpi_call 2 12 "$write", "   Output: " {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "test_disp.v";
    "disp.v";
